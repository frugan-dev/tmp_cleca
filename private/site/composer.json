        "cohensive/oembed": "^0.19.0",
        "creitive/breadcrumbs": "^3.3",
        "davidgorges/color-contrast": "dev-master",
        "egulias/email-validator": "^4.0",
        "ezyang/htmlpurifier": "^4.14",
        "frugan-dev/debugbar-redbean": "^0.1.0",
        "gabordemooij/redbean": "^5.5",
        "giggsey/libphonenumber-for-php": "^9.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.4",
        "http-interop/http-factory-guzzle": "^1.0",
        "illuminate/config": "^12.19",
        "kint-php/kint": "^6.0",
        "laminas/laminas-authentication": "^2.8",
        "laminas/laminas-diactoros": "^3.0",
        "laminas/laminas-eventmanager": "^3.2",
        "laminas/laminas-permissions-rbac": "^3.1",
        "laminas/laminas-servicemanager": "^3.23",
        "laminas/laminas-session": "^2.10",
        "laminas/laminas-stdlib": "^3.6",
        "league/flysystem": "^3.16",
        "league/flysystem-ziparchive": "^3.16",
        "middlewares/client-ip": "^2.0",
        "middlewares/debugbar": "^2.0",
        "middlewares/https": "^2.0.0",
        "middlewares/negotiation": "^2.0",
        "middlewares/shutdown": "^2.0",
        "middlewares/trailing-slash": "^2.0",
        "middlewares/whoops": "^2.0",
        "middlewares/www": "^2.0",
        "monolog/monolog": "^3.2",
        "nesbot/carbon": "^3.2",
        "nette/finder": "^3.0",
        "nette/utils": "^4.0",
        "nyholm/psr7-server": "^1.0",
        "opis/closure": "^4.3",
        "php-debugbar/php-debugbar": "^2.1",
        "php-di/php-di": "^7.0",
        "php-http/httplug": "^2.2",
        "phpoffice/phpspreadsheet": "^5.0",
        "sentry/sentry": "^4.0",
        "shudrum/array-finder": "^1.1",
        "slim/http": "^1.3",
        "slim/http-cache": "^1.1",
        "slim/psr7": "^1.1",
        "slim/slim": "^4.0",
        "soundasleep/html2text": "^2.0",
        "spatie/array-to-xml": "^3.1",
        "ssnepenthe/color-utils": "^0.4.2",
        "symfony/asset": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/http-client": "*",
        "symfony/http-foundation": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/yaml": "^7.0",
        "thecodingmachine/safe": "^3.3",
        "umpirsky/country-list": "^2.0",
        "umpirsky/language-list": "^1.1",
        "vaimo/composer-patches": "^5.1",
        "vlucas/phpdotenv": "^5.1",
        "voku/html-min": "^4.4",
        "wazaari/monolog-mysql": "dev-php8"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.15",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laminas/laminas-code": "^4.7",
        "nikic/php-parser": "^5",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpro/grumphp-shim": "^2.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.3",
        "povils/phpmnd": "^3.0",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "thecodingmachine/phpstan-safe-rule": "^1.3",
        "vimeo/psalm": "^6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ybelenko/monolog-mysql"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "phpro/grumphp-shim": true,
            "vaimo/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "patches-file": "patches.json"
    },
    "scripts": {
        "post-install-cmd": [
            "@post-cmd",
            "@php -f cli/app.php -d memory_limit=-1 -- -p=enabled/install"
        ],
        "post-update-cmd": [
            "@post-cmd",
            "@php -f cli/app.php -d memory_limit=-1 -- -p=enabled/update"
        ],
        "analysis": "@check --tasks=phpstan,psalm",
        "check": "@php -d max_execution_time=0 -d memory_limit=-1 -f ./vendor/bin/grumphp -- run",
        "ci": "@check --no-interaction",
        "lint": "@check --tasks=phpcsfixer,phplint,rector",
        "post-cmd": [
            "./cli/symlink.sh",
            "echo $COMPOSER_DEV_MODE",
            "[ $COMPOSER_DEV_MODE -eq 1 ] || composer dump-autoload --optimize"
        ],
        "quality": "@check --tasks=phpmnd,phpparser",
        "rector": "@php -d max_execution_time=0 -d memory_limit=-1 -f ./vendor/bin/rector -- process --ansi --clear-cache",
        "security": "@check --tasks=securitychecker_roave",
        "test": "@php -d xdebug.mode=off ./vendor/bin/phpunit"
    }
}
