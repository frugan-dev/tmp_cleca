  "main": "index.js",
  "scripts": {
    "develop": "webpack --mode=development --progress",
    "production": "webpack --mode=production --progress",
    "watch": "webpack --mode=development --watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "npx eslint src/js/**",
    "lint:css": "npx stylelint src/{scss,sprite}/**",
    "analyzer": "mkdirp dist/analyzer && npm run analyzer:json && npm run analyzer:bundle && rm dist/analyzer/stats.json",
    "analyzer:json": "webpack --profile --json > dist/analyzer/stats.json",
    "analyzer:bundle": "webpack-bundle-analyzer dist/analyzer/stats.json ../../../public/asset/front --mode static --report dist/analyzer/report.html"
  },
